<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="FontFamilies\Montserrat-Medium.ttf" />
    <None Remove="FontFamilies\Pretendard-Medium.ttf" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="FontFamilies\Montserrat-Medium.ttf" />
    <Resource Include="FontFamilies\Pretendard-Medium.ttf" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="CommunityToolkit.Mvvm" Version="8.3.2" />
  </ItemGroup>

</Project>
